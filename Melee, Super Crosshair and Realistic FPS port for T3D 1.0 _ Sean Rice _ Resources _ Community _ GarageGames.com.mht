        // Apply damage targetted object
   		// Works for all shapebase objects.
        if (%target.getState() !$=3D &quot;Dead&quot; &amp;&amp; %target=
.getId() !$=3D %player.getId())
   		{
   		   // shields and sword rebounding - JM The_Force
