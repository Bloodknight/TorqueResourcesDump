              }
              return;
           }
           //AISK Changes: Start

           //If friendly fire is turned off, and the source and target a=
re on
           //the same team, then return
           if ($AISK_FRIENDLY_FIRE =3D=3D false &amp;&amp; $AISK_FREE_FO=
R_ALL =3D=3D false)
           {
                if (%target.team =3D=3D %player.team)
                    return;
