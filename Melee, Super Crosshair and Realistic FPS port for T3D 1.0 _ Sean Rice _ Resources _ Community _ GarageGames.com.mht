(%dif, 1) @ &quot; 0&quot;;
              %dif        =3D VectorNormalize(%dif);
              %dot        =3D VectorDot(%dif, %objDir2D);
              // 120 Deg angle test...
              // 1.05 =3D=3D 60 degrees in radians
              if (%dot &gt;=3D mCos(1.05))
